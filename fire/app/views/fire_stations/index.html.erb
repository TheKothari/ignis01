<p id="notice"><%= notice %></p>

<h1>Fire Stations</h1>

<table>
  <thead>
    <tr>
      <th>Name|</th>
      <th>Fid|</th>
      <th>Address|</th>
      <th>Bays|</th>
      <th>Houbli|</th>
      <th>Taluk|</th>
      <th>District|</th>
      <th>Range|</th>
      <th>Zone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fire_stations.each do |fire_station| %>
      <tr>
        <td><%= fire_station.Name %></td>
        <td><%= fire_station.FID %></td>
        <td><%= fire_station.Address %></td>
        <td><%= fire_station.Bays %></td>
        <td><%= fire_station.Houbli %></td>
        <td><%= fire_station.Taluk %></td>
        <td><%= fire_station.District %></td>
        <td><%= fire_station.Range %></td>
        <td><%= fire_station.Zone %></td>
        <td><%= link_to 'Show', fire_station %></td>
        <td><%= link_to 'Edit', edit_fire_station_path(fire_station) %></td>
        <td><%= link_to 'Destroy', fire_station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fire Station', new_fire_station_path %><br>
<%= link_to 'offices', offices_path %><br>
<%= link_to 'Other Buildings', other_buildings_path %>
