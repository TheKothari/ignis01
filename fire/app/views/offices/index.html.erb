<p id="notice"><%= notice %></p>

<h1>Offices</h1>

<table>
  <thead>
    <tr>
      <th>Designation|</th>
      <th>Officeno|</th>
      <th>Status|</th>
      <th>Bhk|</th>
      <th>Fid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.Designation %></td>
        <td><%= office.OfficeNo %></td>
        <td><%= office.Status %></td>
        <td><%= office.bhk %></td>
        <td><%= office.FID %></td>
        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Office', new_office_path %><br>
